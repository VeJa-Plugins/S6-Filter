@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix modgui: <http://moddevices.com/ns/modgui#>.
@prefix mod: <http://moddevices.com/ns/mod#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .

<http://VeJaPlugins.com/plugins/Release/m8vcf> 
a lv2:Plugin, mod:ControlVoltagePlugin;
lv2:requiredFeature urid:map ;

doap:name "M8 VCF";

doap:developer [
    foaf:name "Jarno & Jan";
    foaf:homepage <>;
    foaf:mbox <mailto:jan@moddevices.com>;
    ];

doap:maintainer [
    foaf:name "J&J";
    foaf:homepage <>;
    foaf:mbox <mailto:jan@moddevices.com>;
    ];

    lv2:minorVersion 1;
    lv2:microVersion 1;

rdfs:comment """
CV module that contains the VCF used inside the famous Jupiter-8

This module is part of the Mars-8 project 
""";

lv2:port
[
    a lv2:InputPort, lv2:AudioPort;
    lv2:index 0;
    lv2:symbol "input";
    lv2:name "Input";
]
,
[
    a lv2:InputPort, lv2:CVPort, mod:CVPort;
    lv2:index 1;
    lv2:symbol "CvEnvInput";
    lv2:name "CV Env Input";
]
,
[
    a lv2:InputPort, lv2:CVPort, mod:CVPort;
    lv2:index 2;
    lv2:symbol "CvLFOInput";
    lv2:name "CV LFO Input";
]
,
[
    a lv2:InputPort, lv2:CVPort, mod:CVPort;
    lv2:index 3;
    lv2:symbol "CvPitchInput";
    lv2:name "CV Pitch Input";
]
,
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 4;
    lv2:symbol "output";
    lv2:name "Output";
]
,
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 5;
    lv2:symbol "Resonance";
    lv2:name "Resonance";
    lv2:default 0.5 ;
    lv2:minimum 0.001 ;
    lv2:maximum 0.99 ;
]
,
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 6;
    lv2:symbol "CutoffFreq";
    lv2:name "Cutoff Frequency";
    lv2:default 100.0 ;
    lv2:minimum 1.0 ;
    lv2:maximum 20000.0 ;
]
,
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 7;
    lv2:symbol "Slope";
    lv2:name "Slope";
    lv2:portProperty lv2:enumeration , lv2:integer ;
    lv2:default 0 ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:scalePoint
    [
        rdfs:label "-12dB" ;
        rdf:value 0 
    ] , [
        rdfs:label "-24dB" ;
        rdf:value 1 
    ]
]
,
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 8;
    lv2:symbol "LFO_MOD";
    lv2:name "LFO modulation";
    lv2:default 0.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 10.0 ;
]
,
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 9;
    lv2:symbol "ENV_MOD";
    lv2:name "Envelope modulation";
    lv2:default 0.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 10.0 ;
]
,
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 10;
    lv2:symbol "PITCH_MOD";
    lv2:name "Key Follow";
    lv2:default 0.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 10.0 ;
]
.
